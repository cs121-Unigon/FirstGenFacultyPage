<p id="notice"><%= notice %></p>

<h1>Profs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Interests</th>
      <th>Office</th>
      <th>Department</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profs.each do |prof| %>
      <tr>
        <td><%= prof.name %></td>
        <td><%= prof.email %></td>
        <td><%= prof.interests %></td>
        <td><%= prof.office %></td>
        <td><%= prof.department.name %></td>
        <td><%= link_to 'Show', prof %></td>
        <td><%= link_to 'Edit', edit_prof_path(prof) %></td>
        <td><%= link_to 'Destroy', prof, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prof', new_prof_path %>
